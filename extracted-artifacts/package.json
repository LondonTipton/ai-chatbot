{
    "name": "extracted-artifacts",
    "version": "1.0.0",
    "description": "Portable artifact/document side panel component extracted from ai-chatbot",
    "main": "index.ts",
    "type": "module",
    "scripts": {
        "build": "tsc",
        "dev": "tsc --watch",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "artifacts",
        "document-editor",
        "side-panel",
        "react",
        "typescript"
    ],
    "author": "Extracted from Vercel AI Chatbot",
    "license": "MIT",
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    },
    "dependencies": {
        "@radix-ui/react-slot": "^1.0.2",
        "@radix-ui/react-tooltip": "^1.0.7",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.0.0",
        "date-fns": "^3.0.0",
        "fast-deep-equal": "^3.1.3",
        "framer-motion": "^11.0.0",
        "swr": "^2.2.4",
        "tailwind-merge": "^2.0.0",
        "usehooks-ts": "^2.9.1"
    },
    "devDependencies": {
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "typescript": "^5.0.0"
    },
    "files": [
        "extracted-components/**/*",
        "extracted-hooks/**/*",
        "extracted-artifacts/**/*",
        "extracted-utils/**/*",
        "ui/**/*",
        "index.ts",
        "README.md"
    ],
    "exports": {
        ".": {
            "import": "./index.ts",
            "require": "./index.ts"
        },
        "./components/*": {
            "import": "./extracted-components/*",
            "require": "./extracted-components/*"
        },
        "./hooks/*": {
            "import": "./extracted-hooks/*",
            "require": "./extracted-hooks/*"
        },
        "./ui/*": {
            "import": "./ui/*",
            "require": "./ui/*"
        },
        "./utils/*": {
            "import": "./extracted-utils/*",
            "require": "./extracted-utils/*"
        }
    }
}